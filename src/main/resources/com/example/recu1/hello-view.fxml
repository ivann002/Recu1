    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="32.0" prefWidth="389.0">
      <children>
      
          <Label fx:id="welcomeText" prefHeight="22.0" prefWidth="73.0" text="Nombre" />
         <TextField />
      </children>
   </HBox>
   <HBox prefHeight="39.0" prefWidth="389.0">
      <children>
         <Label fx:id="welcomeText1" prefHeight="22.0" prefWidth="73.0" text="Apellidos" />
         <TextField />
      </children>
   </HBox>
   <Button id="Enviar" fx:id="enviar" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Enviar Datos" />
